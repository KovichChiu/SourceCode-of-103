import java.util.Random;
public class HW_002 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Random ran = new Random();
		int [] a = new int [6];
		for(int i=0;i<6;i++){
			a[i] = ran.nextInt(42)+1;
			System.out.print(a[i]+"\t");
		}
		
	}
}
