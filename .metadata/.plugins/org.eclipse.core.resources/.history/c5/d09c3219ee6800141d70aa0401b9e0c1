import java.util.Scanner;
public class HW_001 {
	public static void main(String[] args) {
		Scanner scn = new Scanner(System.in);
		String [] num = {"01272636","39412201","94586356"};
		System.out.print("請輸入一組發票的號碼：");
		String a = scn.next();
		char [] data = a.toCharArray();
		char [] num1 = num[0].toCharArray();
		char [] num2 = num[1].toCharArray();
		char [] num3 = num[2].toCharArray();
		if(data[7]==num1[7]){
			if(data[6]==num1[6]){
				if(data[5]==num1[5]){
					if(data[4]==num1[4]){
						if(data[3]==num1[3]){
							if(data[2]==num1[2]){
								if(data[1]==num1[1]){
									if(data[0]==num1[0]){
										System.out.print("恭喜你中了200000元！");
									}else{
										System.out.print("恭喜你中了40000元！");
									}
								}else{
									System.out.print("恭喜你中了10000元！");
								}
							}else{
								System.out.print("恭喜你中了4000元！");
							}
						}else{
							System.out.print("恭喜你中了1000元！");
						}
					}else{
						System.out.print("恭喜你中了200元！");
					}
				}else{
					System.out.print("很抱歉沒有中獎！");
				}
			}else{
				System.out.print("很抱歉沒有中獎！");
			}
		}
		else if(data[7]==num2[7]){
			if(data[6]==num2[6]){
				if(data[5]==num2[5]){
					if(data[4]==num2[4]){
						if(data[3]==num2[3]){
							if(data[2]==num2[2]){
								if(data[1]==num2[1]){
									if(data[0]==num2[0]){
										System.out.print("恭喜你中了200000元！");
									}else{
										System.out.print("恭喜你中了40000元！");
									}
								}else{
									System.out.print("恭喜你中了10000元！");
								}
							}else{
								System.out.print("恭喜你中了4000元！");
							}
						}else{
							System.out.print("恭喜你中了1000元！");
						}
					}else{
						System.out.print("恭喜你中了200元！");
					}
				}else{
					System.out.print("很抱歉沒有中獎！");
				}
			}else{
				System.out.print("很抱歉沒有中獎！");
			}
		}
		else if(data[7]==num3[7]){
			if(data[6]==num3[6]){
				if(data[5]==num3[5]){
					if(data[4]==num3[4]){
						if(data[3]==num3[3]){
							if(data[2]==num3[2]){
								if(data[1]==num3[1]){
									if(data[0]==num3[0]){
										System.out.print("恭喜你中了200000元！");
									}else{
										System.out.print("恭喜你中了40000元！");
									}
								}else{
									System.out.print("恭喜你中了10000元！");
								}
							}else{
								System.out.print("恭喜你中了4000元！");
							}
						}else{
							System.out.print("恭喜你中了1000元！");
						}
					}else{
						System.out.print("恭喜你中了200元！");
					}
				}else{
					System.out.print("很抱歉沒有中獎！");
				}
			}else{
				System.out.print("很抱歉沒有中獎！");
			}
		}else{
			System.out.print("很抱歉沒有中獎！");
		}
	}

}
